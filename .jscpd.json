{
  "threshold": 30,
  "reporters": ["html", "json", "console"],
  "output": "./reports/duplication",
  "ignore": [
    "**/node_modules/**",
    "**/__pycache__/**",
    "**/.git/**",
    "**/dist/**",
    "**/build/**",
    "**/coverage/**",
    "**/reports/**",
    "**/.pytest_cache/**",
    "**/venv/**",
    "**/env/**",
    "**/tests/fixtures/**",
    "**/tests/_fixtures/**",
    "**/tests/_helpers/**",
    "**/tests/_plugins/**",
    "**/eval/episodes/**",
    "**/test_episodes/**",
    "**/load_tests/**",
    "**/perf/**",
    "**/load/**",
    "**/infra/**",
    "**/k8s/**",
    "**/observability/dashboards/**",
    "**/docs/**",
    "**/LICENSE_HEADER.txt",
    "**/constraints.txt",
    "**/requirements*.txt",
    "**/Dockerfile*",
    "**/docker-compose*.yml",
    "**/start.sh",
    "**/pytest.ini",
    "**/pyproject.toml",
    "**/mypy.ini",
    "**/.ruff.toml",
    "**/.eslintrc.*",
    "**/tsconfig.json",
    "**/vite.config.*",
    "**/tailwind.config.*",
    "**/package*.json"
  ],
  "absolute": true,
  "gitignore": true,
  "minLines": 5,
  "minTokens": 30,
  "mode": "strict",
  "silent": false,
  "debug": false,
  "format": [
    "python",
    "javascript",
    "typescript",
    "tsx",
    "jsx",
    "markdown",
    "yaml",
    "yml"
  ],
  "pattern": [
    "**/*.py",
    "**/*.js",
    "**/*.ts",
    "**/*.tsx",
    "**/*.jsx",
    "**/*.md",
    "**/*.yaml",
    "**/*.yml"
  ]
}
