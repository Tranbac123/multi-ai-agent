{
  "dashboard": {
    "id": null,
    "title": "SLO Monitoring & Error Budgets",
    "tags": ["slo", "error-budget", "monitoring"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "API Availability SLO",
        "type": "graph",
        "targets": [
          {
            "expr": "api_availability_current",
            "legendFormat": "Current Availability"
          },
          {
            "expr": "api_availability_target",
            "legendFormat": "Target (99.9%)"
          }
        ],
        "yAxes": [
          {
            "label": "Availability %",
            "min": 99.0,
            "max": 100.0
          }
        ],
        "thresholds": [
          {
            "value": 99.9,
            "colorMode": "critical",
            "op": "lt"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "API Error Budget",
        "type": "graph",
        "targets": [
          {
            "expr": "api_availability_error_budget",
            "legendFormat": "Error Budget Remaining"
          }
        ],
        "yAxes": [
          {
            "label": "Error Budget %",
            "min": 0,
            "max": 100
          }
        ],
        "thresholds": [
          {
            "value": 20,
            "colorMode": "critical",
            "op": "lt"
          },
          {
            "value": 50,
            "colorMode": "warning",
            "op": "lt"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "API Latency SLO (p95)",
