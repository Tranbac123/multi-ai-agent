          {
            "expr": "api_latency_p95_current",
            "legendFormat": "Current p95 Latency"
          },
          {
            "expr": "api_latency_p95_target",
            "legendFormat": "Target (500ms)"
          }
        ],
        "yAxes": [
          {
            "label": "Milliseconds",
            "min": 0,
            "max": 1000
          }
        ],
        "thresholds": [
          {
            "value": 500,
            "colorMode": "critical",
            "op": "gt"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
