      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {},
    "templating": {
      "list": [
        {
          "name": "operation_type",
          "type": "query",
          "query": "label_values(timeout_events_total, operation_type)",
          "refresh": 1,
          "includeAll": true,
          "multi": true
        },
        {
          "name": "strategy",
          "type": "query",
          "query": "label_values(hedged_requests_total, strategy)",
          "refresh": 1,
          "includeAll": true,
          "multi": true
        },
        {
          "name": "severity",
          "type": "query",
          "query": "label_values(timeout_events_total, severity)",
          "refresh": 1,
          "includeAll": true,
          "multi": true
        }
      ]
    },
    "annotations": {
      "list": [
        {
          "name": "High Timeout Events",
          "datasource": "prometheus",
          "enable": true,
          "expr": "rate(timeout_events_total{job=\"timeout-manager\"}[5m]) > 10",
          "iconColor": "red",
          "tags": ["timeout", "performance"]
        },
        {
          "name": "Hedge Win Events",
          "datasource": "prometheus",
          "enable": true,
          "expr": "changes(hedge_wins_total{job=\"request-hedging\"}[1m])",
          "iconColor": "green",
          "tags": ["hedging", "optimization"]
        },
        {
          "name": "Cancellation Events",
          "datasource": "prometheus",
          "enable": true,
          "expr": "changes(cancellation_requests_total{job=\"coordinated-cancellation\"}[1m])",
          "iconColor": "orange",
          "tags": ["cancellation", "coordination"]
        }
      ]
    },
