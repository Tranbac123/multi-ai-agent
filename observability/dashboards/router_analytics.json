{
  "dashboard": {
    "id": null,
    "title": "Router Analytics Dashboard",
    "tags": ["router", "analytics", "performance"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Success Rate",
        "type": "stat",
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "router_success_rate{tenant_id=\"$tenant_id\"}",
            "legendFormat": "Success Rate",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 1,
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 0.8},
                {"color": "green", "value": 0.95}
              ]
            }
          }
        }
      },
      {
        "id": 2,
        "title": "Decision Latency",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 6, "y": 0},
        "targets": [
          {
            "expr": "histogram_quantile(0.50, router_decision_latency_ms{tenant_id=\"$tenant_id\"})",
            "legendFormat": "P50",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, router_decision_latency_ms{tenant_id=\"$tenant_id\"})",
            "legendFormat": "P95",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, router_decision_latency_ms{tenant_id=\"$tenant_id\"})",
            "legendFormat": "P99",
            "refId": "C"
          }
        ],
        "yAxes": [
          {
            "label": "Latency (ms)",
            "min": 0
          }
        ]
      },
      {
        "id": 3,
        "title": "Tier Distribution",
        "type": "piechart",
        "gridPos": {"h": 8, "w": 6, "x": 18, "y": 0},
        "targets": [
          {
            "expr": "router_tier_distribution{tenant_id=\"$tenant_id\", tier=\"A\"}",
            "legendFormat": "Tier A",
            "refId": "A"
          },
          {
            "expr": "router_tier_distribution{tenant_id=\"$tenant_id\", tier=\"B\"}",
            "legendFormat": "Tier B",
            "refId": "B"
          },
          {
            "expr": "router_tier_distribution{tenant_id=\"$tenant_id\", tier=\"C\"}",
            "legendFormat": "Tier C",
            "refId": "C"
          }
        ]
      },
      {
        "id": 4,
        "title": "Router Misroute Rate",
        "type": "stat",
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "router_misroute_rate{tenant_id=\"$tenant_id\"}",
            "legendFormat": "Misroute Rate",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 1,
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 0.05},
                {"color": "red", "value": 0.1}
              ]
            }
          }
        }
      },
      {
        "id": 5,
        "title": "Expected vs Actual Cost",
        "type": "stat",
        "gridPos": {"h": 8, "w": 6, "x": 6, "y": 8},
        "targets": [
          {
            "expr": "router_expected_vs_actual_cost{tenant_id=\"$tenant_id\"}",
            "legendFormat": "Cost Ratio",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "min": 0,
            "max": 2,
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 0.8},
                {"color": "red", "value": 1.2}
              ]
            }
          }
        }
      },
      {
        "id": 6,
        "title": "Token Usage",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
        "targets": [
          {
            "expr": "router_tokens_in{tenant_id=\"$tenant_id\"}",
            "legendFormat": "Tokens In",
            "refId": "A"
          },
          {
            "expr": "router_tokens_out{tenant_id=\"$tenant_id\"}",
            "legendFormat": "Tokens Out",
            "refId": "B"
          }
        ],
        "yAxes": [
          {
            "label": "Tokens",
            "min": 0
          }
        ]
      },
      {
        "id": 7,
        "title": "Cost per Run",
        "type": "stat",
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 16},
        "targets": [
          {
            "expr": "router_cost_per_run{tenant_id=\"$tenant_id\"}",
            "legendFormat": "Cost per Run",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "currencyUSD",
            "min": 0
          }
        }
      },
      {
        "id": 8,
        "title": "Request Volume",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 6, "y": 16},
        "targets": [
          {
            "expr": "rate(router_requests_total{tenant_id=\"$tenant_id\"}[5m])",
            "legendFormat": "Requests/sec",
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Requests/sec",
            "min": 0
          }
        ]
      },
      {
        "id": 9,
        "title": "Error Rate by Tier",
        "type": "graph",
        "gridPos": {"h": 8, "w": 6, "x": 18, "y": 16},
        "targets": [
          {
            "expr": "rate(router_errors_total{tenant_id=\"$tenant_id\", tier=\"A\"}[5m])",
            "legendFormat": "Tier A Errors",
            "refId": "A"
          },
          {
            "expr": "rate(router_errors_total{tenant_id=\"$tenant_id\", tier=\"B\"}[5m])",
            "legendFormat": "Tier B Errors",
            "refId": "B"
          },
          {
            "expr": "rate(router_errors_total{tenant_id=\"$tenant_id\", tier=\"C\"}[5m])",
            "legendFormat": "Tier C Errors",
            "refId": "C"
          }
        ],
        "yAxes": [
          {
            "label": "Errors/sec",
            "min": 0
          }
        ]
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "30s",
    "schemaVersion": 27,
    "version": 0,
    "links": [],
    "templating": {
      "list": [
        {
          "name": "tenant_id",
          "type": "textbox",
          "label": "Tenant ID",
          "query": "default_tenant",
          "current": {
            "text": "default_tenant",
            "value": "default_tenant"
          }
        }
      ]
    }
  }
}
