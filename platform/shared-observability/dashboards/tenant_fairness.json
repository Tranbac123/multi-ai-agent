          "y": 20
        }
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {},
    "templating": {
      "list": [
        {
          "name": "tenant",
          "type": "query",
          "query": "label_values(tenant_tokens_in_flight, tenant_id)",
          "refresh": 1,
          "includeAll": true,
          "multi": true
        },
        {
          "name": "plan",
          "type": "query",
          "query": "label_values(tenant_tokens_in_flight, plan)",
          "refresh": 1,
          "includeAll": true,
          "multi": true
        }
      ]
    },
    "annotations": {
      "list": [
        {
          "name": "Degradation Events",
          "datasource": "prometheus",
          "enable": true,
          "expr": "changes(degradation_mode{job=\"orchestrator\"}[1m])",
          "iconColor": "orange",
          "tags": ["degradation", "system-load"]
        },
        {
          "name": "High Rejection Rate",
          "datasource": "prometheus",
          "enable": true,
          "expr": "admission_rate{job=\"api-gateway\"} < 90",
          "iconColor": "red",
          "tags": ["admission", "rejection"]
        }
      ]
    },
    "refresh": "5s",
    "schemaVersion": 27,
    "version": 1,
    "links": []
  }
}
