    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {},
    "templating": {
      "list": [
        {
          "name": "component_name",
          "type": "query",
          "query": "label_values(component_vulnerability_count, component_name)",
          "refresh": 1,
          "includeAll": true,
          "multi": true
        },
        {
          "name": "severity",
          "type": "query",
          "query": "label_values(vulnerability_severity_counts, severity)",
          "refresh": 1,
          "includeAll": true,
          "multi": true
        },
        {
          "name": "rule_id",
          "type": "query",
          "query": "label_values(cve_gate_status, rule_id)",
          "refresh": 1,
          "includeAll": true,
          "multi": true
        },
        {
          "name": "slsa_level",
          "type": "query",
          "query": "label_values(slsa_provenance_levels, level)",
          "refresh": 1,
          "includeAll": true,
          "multi": true
        }
      ]
    },
    "annotations": {
      "list": [
        {
          "name": "CVE Gate Failures",
          "datasource": "prometheus",
          "enable": true,
          "expr": "changes(cve_gate_failures_total{job=\"cve-gates\"}[1m])",
          "iconColor": "red",
          "tags": ["cve", "gate", "failure"]
        },
        {
          "name": "High Severity Vulnerabilities",
          "datasource": "prometheus",
          "enable": true,
          "expr": "vulnerability_severity_counts{job=\"cve-gates\", severity=\"critical\"} > 0",
          "iconColor": "red",
          "tags": ["vulnerability", "critical"]
        },
        {
          "name": "Image Signing Events",
          "datasource": "prometheus",
          "enable": true,
          "expr": "changes(image_signing_events_total{job=\"image-signing\"}[1m])",
          "iconColor": "blue",
          "tags": ["image", "signing"]
        },
        {
          "name": "SBOM Generation Events",
          "datasource": "prometheus",
          "enable": true,
          "expr": "changes(sbom_generation_total{job=\"sbom-generator\"}[1m])",
          "iconColor": "green",
          "tags": ["sbom", "generation"]
        }
      ]
    },
    "refresh": "5s",
    "schemaVersion": 27,
    "version": 1,
