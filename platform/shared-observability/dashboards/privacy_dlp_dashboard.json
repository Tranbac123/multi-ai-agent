      "to": "now"
    },
    "timepicker": {},
    "templating": {
      "list": [
        {
          "name": "tenant",
          "type": "query",
          "query": "label_values(pii_detections_total, tenant_id)",
          "refresh": 1,
          "includeAll": true,
          "multi": true
        },
        {
          "name": "pii_type",
          "type": "query",
          "query": "label_values(pii_detections_total, pii_type)",
          "refresh": 1,
          "includeAll": true,
          "multi": true
        },
        {
          "name": "sensitivity_level",
          "type": "query",
          "query": "label_values(sensitivity_tags_total, sensitivity_level)",
          "refresh": 1,
          "includeAll": true,
          "multi": true
        }
      ]
    },
    "annotations": {
      "list": [
        {
          "name": "Privacy Violations",
          "datasource": "prometheus",
          "enable": true,
          "expr": "changes(privacy_violations_total{job=\"privacy-middleware\"}[1m])",
          "iconColor": "red",
          "tags": ["privacy", "violation"]
        },
        {
          "name": "Key Rotations",
          "datasource": "prometheus",
          "enable": true,
          "expr": "changes(key_rotations_total{job=\"field-encryption\"}[1m])",
          "iconColor": "blue",
          "tags": ["encryption", "key-rotation"]
        },
        {
          "name": "High PII Detection",
          "datasource": "prometheus",
          "enable": true,
          "expr": "rate(pii_detections_total{job=\"privacy-middleware\"}[5m]) > 10",
          "iconColor": "orange",
          "tags": ["pii", "detection"]
        }
      ]
    },
