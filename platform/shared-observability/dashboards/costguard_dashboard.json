    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {},
    "templating": {
      "list": [
        {
          "name": "tenant",
          "type": "query",
          "query": "label_values(tenant_budget_usage_percent, tenant_id)",
          "refresh": 1,
          "includeAll": true,
          "multi": true
        },
        {
          "name": "service_type",
          "type": "query",
          "query": "label_values(cost_drift_percent, service_type)",
          "refresh": 1,
          "includeAll": true,
          "multi": true
        }
      ]
    },
    "annotations": {
      "list": [
        {
          "name": "Budget Alerts",
          "datasource": "prometheus",
          "enable": true,
          "expr": "changes(budget_alerts_total{job=\"billing-service\"}[1m])",
          "iconColor": "red",
          "tags": ["budget", "alert"]
        },
        {
          "name": "Cost Drift Events",
          "datasource": "prometheus",
          "enable": true,
          "expr": "changes(cost_drift_percent{job=\"router_service\"}[1m]) > 20",
          "iconColor": "orange",
          "tags": ["drift", "cost"]
        },
        {
          "name": "Safe Mode Activations",
          "datasource": "prometheus",
          "enable": true,
          "expr": "changes(safe_mode_level{job=\"router_service\"}[1m])",
          "iconColor": "yellow",
          "tags": ["safe-mode", "optimization"]
        }
      ]
    },
    "refresh": "5s",
    "schemaVersion": 27,
    "version": 1,
    "links": []
